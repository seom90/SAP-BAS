{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run manageproducts-webapp",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/manageproducts-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/opensapwebapp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/webapp",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/manageproducts/webapp\"}"
      },
      "envFile": "${workspaceFolder}/webapp/.env1",
      "preLaunchTask": "Build manageproducts"
    }
  ]
}